<!DOCTYPE html>
<html>
  <head>
    <title>Wintergreen</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Lato:400,700|Roboto+Mono', media: 'all' %>
    <%= stylesheet_link_tag 'https://npmcdn.com/tachyons@4.1.0/css/tachyons.min.css', media: 'all' %>
    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render partial: "shared/nav" %>
    <%= yield %>
    <%= render partial: "shared/footer" %>
  </body>
</html>
